<class-descriptor
     class="org.realtor.rets.dataobjects.Office"
     table="OFFICES"
>
   <field-descriptor
      name="oid"
      column="OID"
      jdbc-type="INTEGER"
      primarykey="true"
      autoincrement="true"
      access="anonymous"
   />
   <field-descriptor
      name="address1"
      column="ADDRESS1"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="address2"
      column="ADDRESS2"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="city"
      column="CITY"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="faxNumber"
      column="FAX"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="lastModified"
      column="MODIFIED"
      jdbc-type="DATE"
   />
   <field-descriptor
      name="officeMail"
      column="OFFICEEMAIL"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="id"
      column="OFFICEID"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="officeName"
      column="OFFICENAME"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="officeStatus"
      column="OFFICESTATUS"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="optinFlag"
      column="OPTINFLG"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="phone"
      column="PHONE"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="state"
      column="STATE"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="UID"
      column="UID"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="uidCompany"
      column="UIDCOMPANY"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="webAddress"
      column="WEB"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="zip"
      column="ZIP"
      jdbc-type="VARCHAR"
   />
   <field-descriptor
      name="zipExtension"
      column="ZIP4"
      jdbc-type="VARCHAR"
   />
   <collection-descriptor
   	  name="agents"
   	  element-class-ref="org.realtor.rets.dataobjects.Agent"
   	  indirection-table="OFFICE_AGENTS"
   	  auto-retrieve="true"
   	  auto-update="true"
   	>
   		<fk-pointing-to-this-class column="office_oid" />
   		<fk-pointing-to-element-class column="agent_oid" />
   </collection-descriptor>
   
</class-descriptor>