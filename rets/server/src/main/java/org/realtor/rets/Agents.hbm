<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping package="org.realtor.rets.database">
	<class name="Agents" table="agents" lazy="true">
		<id
			column="OID"
			name="Oid"
			type="java.lang.Long"
		>
			<generator class="vm" />
		</id>
		<property
			column="UIDOFFICE"
			length="50"
			name="Uidoffice"
			not-null="false"
			type="string"
		 />
		<property
			column="STATE"
			length="50"
			name="State"
			not-null="false"
			type="string"
		 />
		<property
			column="MEMBERTYPE"
			length="50"
			name="Membertype"
			not-null="false"
			type="string"
		 />
		<property
			column="MOBILEPHONE"
			length="50"
			name="Mobilephone"
			not-null="false"
			type="string"
		 />
		<property
			column="WEB"
			length="50"
			name="Web"
			not-null="false"
			type="string"
		 />
		<property
			column="LOGINID"
			length="50"
			name="Loginid"
			not-null="false"
			type="string"
		 />
		<property
			column="PERSONALTITLE"
			length="50"
			name="Personaltitle"
			not-null="false"
			type="string"
		 />
		<property
			column="LASTNAME"
			length="50"
			name="Lastname"
			not-null="false"
			type="string"
		 />
		<property
			column="HOMEFAX"
			length="50"
			name="Homefax"
			not-null="false"
			type="string"
		 />
		<property
			column="PREFERREDPHONE"
			length="50"
			name="Preferredphone"
			not-null="false"
			type="string"
		 />
		<property
			column="EMAIL"
			length="50"
			name="Email"
			not-null="false"
			type="string"
		 />
		<property
			column="MODIFIED"
			length="19"
			name="Modified"
			not-null="false"
			type="timestamp"
		 />
		<property
			column="ONLINESTATUS"
			length="50"
			name="Onlinestatus"
			not-null="false"
			type="string"
		 />
		<property
			column="ZIP4"
			length="5"
			name="Zip4"
			not-null="false"
			type="string"
		 />
		<property
			column="ZIP"
			length="10"
			name="Zip"
			not-null="false"
			type="string"
		 />
		<property
			column="MIDDLENAME"
			length="50"
			name="Middlename"
			not-null="false"
			type="string"
		 />
		<property
			column="PREFERREDFAX"
			length="50"
			name="Preferredfax"
			not-null="false"
			type="string"
		 />
		<property
			column="ADDRESS2"
			length="75"
			name="Address2"
			not-null="false"
			type="string"
		 />
		<property
			column="PASSWORD"
			length="15"
			name="Password"
			not-null="false"
			type="string"
		 />
		<property
			column="FULLNAME"
			length="50"
			name="Fullname"
			not-null="false"
			type="string"
		 />
		<property
			column="MLSSTATUS"
			length="50"
			name="Mlsstatus"
			not-null="false"
			type="string"
		 />
		<property
			column="FIRSTNAME"
			length="50"
			name="Firstname"
			not-null="false"
			type="string"
		 />
		<property
			column="UID"
			length="50"
			name="Uid"
			not-null="false"
			type="string"
		 />
		<property
			column="PUBID"
			length="50"
			name="Pubid"
			not-null="false"
			type="string"
		 />
		<property
			column="LICENSENO"
			length="50"
			name="Licenseno"
			not-null="false"
			type="string"
		 />
		<property
			column="HOMEPHONE"
			length="50"
			name="Homephone"
			not-null="false"
			type="string"
		 />
		<property
			column="CITY"
			length="50"
			name="City"
			not-null="false"
			type="string"
		 />
		<property
			column="GENDER"
			length="5"
			name="Gender"
			not-null="false"
			type="string"
		 />
		<property
			column="ADDRESS1"
			length="75"
			name="Address1"
			not-null="false"
			type="string"
		 />
		<property
			column="PAGERNUMBER"
			length="50"
			name="Pagernumber"
			not-null="false"
			type="string"
		 />
		<set name="properties" table="agent_properties" lazy="true">
           <key column="agent_oid"/>
           <many-to-many column="property_oid" class="Properties" outer-join="true"/>
     	</set>
		<set name="offices" table="office_agents" lazy="true">
           <key column="agent_oid"/>
           <many-to-many column="office_oid" class="Offices" outer-join="true"/>
     	</set>
	</class>
</hibernate-mapping>
